@article{solovevaCennostiOYaTKak2019,
  title = {О ценности ОЯТ как сырья для топлива реакторов на тепловых нейтронах},
  author = {Соловьева, Алена Петровна and Ульянин, Юрий Александрович and Харитонов, Владимир Витальевич and Юршина, Дарья Юрьевна},
